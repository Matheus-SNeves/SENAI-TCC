{
  "openapi": "3.0.0",
  "info": {
    "title": "API de Supermercado",
    "description": "Uma API abrangente para gerenciar um sistema de supermercado, incluindo clientes, produtos, pedidos, funcionários e mais.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:1243"
    }
  ],
  "tags": [
    {
      "name": "Clientes",
      "description": "Gerenciamento de clientes"
    },
    {
      "name": "Produtos",
      "description": "Gerenciamento de produtos do supermercado"
    },
    {
      "name": "Pedidos",
      "description": "Gerenciamento de pedidos"
    },
    {
      "name": "Itens do Pedido",
      "description": "Gerenciamento de itens dentro de um pedido"
    },
    {
      "name": "Pagamentos",
      "description": "Gerenciamento de pagamentos"
    },
    {
      "name": "Avaliações",
      "description": "Gerenciamento de avaliações de produtos"
    },
    {
      "name": "Funcionários",
      "description": "Gerenciamento de funcionários e suas funções"
    },
    {
      "name": "Endereços",
      "description": "Gerenciamento de endereços"
    },
    {
      "name": "Empresas",
      "description": "Gerenciamento de empresas"
    },
    {
      "name": "Tipo de Emprego",
      "description": "Gerenciamento de tipos de emprego/cargos"
    },
    {
      "name": "Autenticação",
      "description": "Rotas de login e cadastro"
    }
  ],
  "paths": {
    "/cadastro/cliente": {
      "post": {
        "tags": [
          "Autenticação"
        ],
        "summary": "Cadastrar um novo cliente",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CadastroClienteInput"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Cliente criado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Cliente"
                }
              }
            }
          },
          "409": {
            "description": "Conflito: o CPF já está cadastrado",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Erro interno do servidor"
          }
        }
      }
    },
    "/login/cliente": {
      "post": {
        "tags": [
          "Autenticação"
        ],
        "summary": "Login de cliente",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginInput"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login bem-sucedido",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginOutput"
                }
              }
            }
          },
          "401": {
            "description": "Não autorizado: E-mail ou Senha incorretos"
          },
          "500": {
            "description": "Erro interno do servidor"
          }
        }
      }
    },
    "/login/admin": {
      "post": {
        "tags": [
          "Autenticação"
        ],
        "summary": "Login de administrador (funcionário)",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginInput"
